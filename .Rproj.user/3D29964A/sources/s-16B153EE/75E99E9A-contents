## for testing
n = 100
A0 = GenA(100,20,0.4,0.1)
n0 = dim(A0)[1]; n = dim(A0)[2]
rho = c(0.2,rep(0.8/n0,n0))
pi = rep(0.05,n)
G0 = GenG(A0,1000,rho,pi)$G

## parameter setting
n0 = 30
A = matrix(runif(n0*n),nrow=n0)
diag(A)=1
rho = runif(n0+1)
rho = rho/sum(rho)
pi = colMeans(G0)
lambda = 0.1/n
EM(G0,A,rho,pi,lambda)

